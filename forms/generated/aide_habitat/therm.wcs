    <post_condition>
        <condition>
          <type>python</type>
          <value>not ((form_var_travaux_eligibles_anah if form_var_travaux_eligibles_anah and isinstance(form_var_travaux_eligibles_anah, ("Non").__class__) else "") == ("Non" if "Non" else ""))</value>
